    "#Energy evaluation\n",
    "## for single\n",
    "def en_verl(i,k,ver):\n",
    "    ev=0.\n",
    "    for j in ver[i]:\n",
    "        ev+=u(k,pos[j])/N\n",
    "    return ev\n",
    "\n",
    "#total energy\n",
    "def e_tot_v():\n",
    "    e_v=0\n",
    "    for i in range(N):\n",
    "        e_v+=en_verl(i,pos[i],ver)*0.5\n",
    "    return e_v\n",
    "\n",
    "#classical way\n",
    "def en(pos=pos):\n",
    "    e_p=0\n",
    "    for i in range(N):\n",
    "        for j in range(N):\n",
    "            if i!=j:\n",
    "                e_p+=u(pos[i],pos[j])/N*0.5\n",
    "    return(e_p)\n",
    "\n",
    "#show they are the same \n",
    "print(e_tot_v(),en())\n",
    "e=e_tot_v()"
   ]
