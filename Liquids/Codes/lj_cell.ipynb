      "source": [
        "g=np.zeros(nh+1)\n",
        "ng=0\n",
        "\n",
        "def add_data(g,n_g=ng):\n",
        "    for i in range(N-1):\n",
        "        for j in range(i+1,N):\n",
        "            r=norm_pb(pos[i],pos[j])\n",
        "            if r<L/2:\n",
        "              ig=np.rint(r/delg).astype(int)\n",
        "              g[ig]+=2\n",
        "    return(n_g+1)\n",
        "\n",
        "def g_r(g,ng):\n",
        "    G=g.copy()\n",
        "    for i in range(len(G)):\n",
        "        r=delg*(i+0.5)\n",
        "        vb=((i+1)**3-i**3)*(delg**3)\n",
        "        nid=4/3*np.pi*vb*rho\n",
        "        G[i]=G[i]/(N*nid*ng)\n",
        "    return G\n",
        "\n",
        "ng=add_data(g,ng)\n",
        "gg=g_r(g,ng)\n",
        "plt.plot(rr,gg[:],'-')"
      ],
