    "# Parameter\n",
    "# all in reduced units\n",
    "N=100\n",
    "T=1.5\n",
    "beta=1/T\n",
    "rho=0.9\n",
    "V=N/rho\n",
    "L=V**(1/3)\n",
    "print(rho,L,V)"
   ]
