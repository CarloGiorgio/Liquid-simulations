    "#distance in periodic boundary conditions\n",
    "def norm_pb(_x,_y):\n",
    "    return(norm(_x-(_y+L*np.rint((_x-_y)/L))))\n",
    "    \n",
    "#Lennard Jones potential truncated \n",
    "rc=2.5\n",
    "def u(i,j,r_c=rc):\n",
    "    r=norm_pb(i,j)\n",
    "    if r>r_c: \n",
    "        return 0.\n",
    "    else:        \n",
    "        return 4*((1/r)**(12)-(1/r)**6)"
   ]
  },
