    "def mcmove_dum(e,pos=pos,step=0.1):\n",
    "    for i in range(N):\n",
    "        p=np.random.randint(N)\n",
    "        pos2=pos.copy()\n",
    "        pos2[p]=(pos2[p]+(0.5-np.random.random(3))*step+L)%L\n",
    "        delta=en(pos2)-e\n",
    "        if(np.random.random()<min(1,np.exp(-beta*delta))):\n",
    "            pos=pos2.copy()\n",
    "            e+=delta\n",
    "    \n",
    "    return pos.copy(),e"
   ]
