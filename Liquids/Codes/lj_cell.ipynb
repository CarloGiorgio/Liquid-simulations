    "#Verlet list\n",
    "\n",
    "rv=2.7\n",
    "\n",
    "def verl_list():\n",
    "    posv=pos.copy()\n",
    "    ver=[[]for i in range(N)]\n",
    "    for i in range(N-1):\n",
    "        for j in range(i+1,N):\n",
    "            if norm_pb(pos[i],pos[j])<rv:\n",
    "                ver[i].append(j)\n",
    "                ver[j].append(i)\n",
    "                \n",
    "    return posv,ver\n",
    "\n",
    "posv,ver=verl_list()"
   ]
