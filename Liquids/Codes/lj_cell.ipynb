        "outputId": "e1213bf9-c84c-4880-b0dd-4e1d47632f86"
      },
      "source": [
        "# Parameter\n",
        "# all in reduced units\n",
        "N=100\n",
        "T=1.5\n",
        "beta=1./T\n",
        "rho=0.9\n",
        "V=N/rho\n",
        "L=V**(1/3)\n",
        "print(rho,L,V)"
      ],
      "execution_count": 79,
